commands:
  01-install-node:
    command: "yum install nodejs npm --enablerepo=epel -y || :"

container_commands:
  02-install-node-modules:
    command: "sudo -u webapp npm install --dd --logs-dir=. || :"

container_commands:
  03-npm-build:
    command: "sudo -u webapp npm run build --dd --logs-dir=. || :"
